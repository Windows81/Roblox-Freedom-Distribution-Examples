end

if msg:lower():sub(1,4) == &quot;hat &quot; then
local chk1 = msg:lower():sub(5):find(&quot; &quot;) + 4
local plrz = GetPlr(plr, msg:lower():sub(5, chk1-1))
for i, v in pairs(plrz) do
